<h1>Users</h1>
  
<%= search_form_for @q do |f| %>
  <div >
    <%= f.search_field :username_cont, class:"form-control"%>
    <%= f.submit "Search" %>
  </div>
<% end %>

<div id="users">
  <% @users.each do |user| %>
    <img src="<%= "https://picsum.photos/id/#{user.id}/250/166" %>" alt="User's Picture">
    <%= render user %>
    <%= link_to "Edit", edit_user_path(user) %>
  <% end %>
</div>